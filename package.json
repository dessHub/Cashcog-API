{
  "name": "Cashcog-API",
  "version": "1.0.0",
  "main": "server.js",
  "scripts": {
    "start": "DEBUG=log nodemon --exec babel-node src/server.js",
    "production": "babel-node src/server.js",
    "test": "NODE_ENV=test jest --coverage --no-cache  --detectOpenHandles --runInBand --forceExit",
    "test:coveralls": "nyc --reporter=text-lcov npm test | coveralls"
  },
  "devDependencies": {
    "coveralls": "^3.0.3",
    "jest": "^24.8.0",
    "mongodb-memory-server": "^6.2.4",
    "superagent": "^5.0.5",
    "supertest": "^4.0.2"
  },
  "dependencies": {
    "@babel/core": "^7.4.4",
    "@babel/node": "^7.2.2",
    "@babel/plugin-transform-regenerator": "^7.4.4",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "@babel/runtime": "^7.4.4",
    "babel-polyfill": "^6.26.0",
    "body-parser": "^1.19.0",
    "dotenv": "^8.0.0",
    "express": "^4.16.4",
    "helmet": "^3.18.0",
    "mongoose": "^5.8.9",
    "morgan": "^1.9.1",
    "node-cron": "^2.0.3",
    "node-fetch": "^2.6.0",
    "nyc": "^15.0.0"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "app/**/*.{js}",
      "!server.js",
      "!node_modules/"
    ],
    "coveragePathIgnorePatterns": [
      "<rootDir>/config",
      "<rootDir>/migrations",
      "<rootDir>/seeders",
      "<rootDir>/app/models/index.js"
    ]
  },
  "repository": "https://github.com/dessHub/Cashcog-API.git",
  "author": "desshub <desshub95@gmail.com>",
  "license": "MIT"
}
